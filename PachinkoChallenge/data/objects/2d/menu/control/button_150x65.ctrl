<UIControl>

    <spriteList>

        <sprite objectName="150x65_button_solid">
            <scriptLst>
                <script disable="Control_Hidden"/>
                <script inactive="Control_Hidden"/>
                <script active="Control_Solid_Active"/>
                <script select="Control_Solid_Selected"/>
            </scriptLst>
        </sprite>
    
        <sprite objectName="150x65_button_frame">
            <scriptLst>
                <script disable="Control_Hidden"/>
                <script inactive="Control_Hidden"/>
                <script active="Control_Active"/>
                <script select="Control_Selected"/>
            </scriptLst>
        </sprite>

        <sprite objectName="control_font">
            <font fontName="dejavu_sans_reg_55"/>
            <scriptLst>
                <script disable="Control_Disabled"/>
                <script inactive="Control_Inactive"/>
                <script active="Control_Active"/>
                <script select="Control_Selected_Dispatch_Exe"/>
            </scriptLst>
        </sprite>

    </spriteList>

</UIControl>