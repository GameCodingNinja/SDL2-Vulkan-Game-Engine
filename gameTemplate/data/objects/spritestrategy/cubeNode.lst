<node>

    <node name="cube" type="spriteNode" defaultGroup="(cube)">
        
        <sprite3d objectName="cube">
            <position x="0" y="0" z="0"/>
            <scale x="3" y="3" z="3"/>
            <scriptLst>
                <script rotate="State_RotateCube" group="(state)"/>
            </scriptLst>
        </sprite3d>

    </node>

</node>
