<node>
    
    <node name="loadAnim" type="spriteNode" defaultGroup="(load)">
        
        <sprite2d objectName="loadAnim">
            <dynamicOffset right="150" bottom="150"/>
            <scriptLst>
                <script fadeIn="State_FadeIn"/>
                <script fadeOut="State_FadeOut"/>
                <script loadAnimationLoop="State_LoopAnimation"/>
            </scriptLst>
        </sprite2d>

    </node>

</node>
